{
  "type": "module",
  "name": "motomarine-ui",
  "version": "0.1",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build:library": "vite build --config vite.config.ts",
    "demo": "vite --config vite.demo.config.ts",
    "sandbox": "vite --config vite.sandbox.config.ts",
    "build:sandbox": "vite build --config vite.sandbox.config.ts && cp -r sandbox/assets demo/",
    "build": "npm run build:library && npm run build:sandbox"
  },
  "dependencies": {
    "lit": "^3.2.1",
    "swiper": "^11.1.15"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "postcss-custom-media": "^11.0.5",
    "postcss-import": "^16.1.0",
    "postcss-mixins": "^11.0.3",
    "postcss-nested": "^7.0.2",
    "postcss-pxtorem": "^6.1.0",
    "postcss-url": "^10.1.3",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-string": "^1.2.3"
  }
}
